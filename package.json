{
  "name": "quizz",
  "version": "0.0.0",
  "description": "",
  "main": "build/server/index.js",
  "dependencies": {
    "easyrtc": "^1.0.15",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "immutable": "~3.7.6",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "babel": "~6.3.26",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-tape-runner": "~2.0.0",
    "browserify": "^12.0.1",
    "live-reload": "~1.1.0",
    "nodemon": "~1.8.1",
    "parallelshell": "~2.0.0",
    "rimraf": "~2.5.0",
    "tap-spec": "~4.1.1",
    "tape": "~4.4.0",
    "uglifyjs": "~2.4.10"
  },
  "scripts": {
    "clean": "rimraf dist/* && mkdir dist/public",
    "prebuild": "npm run clean",
    "build:js": "babel src -d dist && browserify dist/client/index.js | uglifyjs > dist/public/client.js",
    "build": "parallelshell 'npm run build:js'",
    "watch:js": "parallelshell 'babel src -w -d dist' ''",
    "watch": "parallelshell 'npm run watch:js'",
    "test": "babel-tape-runner test/**/*.js | tap-spec",
    "live-reload": "live-reload --port 9091 dist/",
    "dev": "parallelshell 'npm run live-reload' 'npm run watch'",
    "start": "node dist/server/index.js"
  },
  "author": "Julien Pradet <julien@pradet.me> (http://julien.pradet.me/)",
  "license": "MIT"
}
