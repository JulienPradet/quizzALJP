{
  "name": "quizz",
  "version": "0.0.0",
  "description": "",
  "main": "build/server/index.js",
  "dependencies": {
    "ejs": "^2.3.*",
    "express": "^4.13.*",
    "immutable": "~3.7.*",
    "peerjs": "^0.3.*",
    "react": "^0.14.*",
    "react-dom": "^0.14.*",
    "react-router": "^1.0.*",
    "history": "^1.17.*",
    "rx": "^4.0.*"
  },
  "devDependencies": {
    "babel": "~6.3.26",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-tape-runner": "~2.0.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "exorcist": "^0.4.0",
    "livereload": "^0.4.0",
    "parallelshell": "~2.0.0",
    "rimraf": "~2.5.0",
    "tap-spec": "~4.1.1",
    "tape": "~4.4.0",
    "uglifyjs": "~2.4.10",
    "watchify": "^3.6.1"
  },
  "scripts": {
    "clean": "rimraf dist && mkdir -p dist && mkdir -p dist/public",
    "prebuild": "npm run clean",
    "build:js": "babel src -d dist && browserify dist/client/index.js > dist/public/client.js",
    "build": "parallelshell 'npm run build:js'",
    "watch:browserify": "watchify -d src/client/index.js -t babelify -o 'exorcist dist/public/client.js.map > dist/public/client.js'",
    "watch": "parallelshell 'npm run watch:browserify'",
    "livereload": "livereload dist/public",
    "dev": "parallelshell 'npm run livereload' 'npm run watch'",
    "test:common": "babel-tape-runner test/common/**/*.test.js | tap-spec",
    "test:client": "mkdir -p testdist/public && babel test/client -d testdist && browserify -t babelify test/client/client.js > testdist/public/client.js && node testdist/index.js",
    "test": "npm run test:common && npm run test:client",
    "serve": "node dist/server/index.js",
    "prestart": "npm run build",
    "start": "npm run serve"
  },
  "author": "Julien Pradet <julien@pradet.me> (http://julien.pradet.me/)",
  "license": "MIT",
  "testling": {
    "files": "test/client/**/*.js",
    "browsers": [
      "chrome/latest"
    ]
  }
}
